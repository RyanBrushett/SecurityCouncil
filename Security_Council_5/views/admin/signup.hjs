<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src='/js/adminFunctions.js'></script>
    <script src='/js/ajax.js'></script>
  </head>
  <body>
  <div id="adminwrap">
    <div id="adminhead">
      <h2><a href="/">S.C. SIM 9000</a></h2>
    </div>
    <div id="adminmain">
<!-- MAIN -->
      <div id="adminmaindisplay">
        {{{ errormessage }}}
        <form action = "/signup" method="POST">
          Full Name: <input type="text" name="fullname" placeholder="John Doe"><br />
          Email: <input type="email" name="username" placeholder="Your Email"><br />
          Password: <input type="password" name="password" placeholder="Password"><br />
          Confirm: <input type="password" name="confirmpass" placeholder="Confirm Pass"><br />
          First Team Preference: <select name="pref1">
            {{{ memberoptions }}}
          </select> <br />
          Second Team Preference: <select name="pref2">
            {{{ memberoptions }}}
          </select> <br />
          Third Team Preference: <select name="pref3">
            {{{ memberoptions }}}
          </select> <br />
          Select the SCSS in which you'll be participating: <select name="scss">
            {{{ roomlistdropdown }}}
          </select> <br />
          <input type ="Submit" value ="Enter">
        </form>
      </div>
<!-- END MAIN -->
      <div id="adminmainfooter">
      </div>
    </div>
    <div id="adminside">
    </div>
  </div>
  </body>
</html>
