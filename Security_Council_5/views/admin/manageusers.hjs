<form method="post" action="/sc-admin/manageusers/getusersbyroom">
    <select name="room" id="room">
        {{{selectedroom}}}
        {{#roomlist}}
            <option value="{{id}}">{{name}}</option>
        {{/roomlist}}
    </select>
    <input type="submit" value="Select Room" />
</form>
<h3>Current Users:</h3>
{{#userlist}}
    <b>Username:</b> <a href="javascript:getUserInfo('{{UserName}}')">{{UserName}}</a> <b>Role:</b> {{Position}} <b>Team:</b> {{Country}}<br />
    <div id="{{UserName}}"></div>
{{/userlist}}

